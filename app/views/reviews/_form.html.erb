<%= form_with(model: [ @plan, Review.new], local: true)  do |form| %>



  <div class="field">
    <%= form.label :rate %>
    <%= form.number_field :rate %>
  </div>

  <div class="field">
    <%= form.label :review %>
    <%= form.text_field :review %>
  </div>

  <div class="actions detail-btn">
    <%= form.submit %>
  </div>

<% end %>